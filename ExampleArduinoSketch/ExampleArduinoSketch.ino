#define  BLACK   0x0000
#define WHITE   0xFFFF

Adafruit_TFTLCD tft(A0, A1, A2, A3, A4);

void setup() {
  // put your setup code here, to run once:
  tft.reset();

  uint16_t identifier = tft.readID();
  
  tft.begin(identifier);
  
  tft.fillScreen(WHITE);
  
  draw_Image(ImgArray);

}

